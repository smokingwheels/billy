        m$ = m$ + "<p>020:027 The heaven shall reveal his iniquity; and the earth shall rise</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;up against him.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>020:028 The increase of his house shall depart   and his goods shall</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;flow away in the day of his wrath.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>020:029 This is the portion of a wicked man from God   and the heritage</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;appointed unto him by God.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:001 But Job answered and said</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:002 Hear diligently my speech   and let this be your consolations.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:003 Suffer me that I may speak; and after that I have spoken   mock</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;on.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:004 As for me   is my complaint to man? and if it were so   why</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;should not my spirit be troubled?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:005 Mark me   and be astonished   and lay your hand upon your mouth.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:006 Even when I remember I am afraid   and trembling taketh hold on</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;my flesh.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:007 Wherefore do the wicked live   become old   yea   are mighty in</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;power?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:008 Their seed is established in their sight with them   and their</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;offspring before their eyes.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:009 Their houses are safe from fear   neither is the rod of God</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;upon them.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:010 Their bull gendereth   and faileth not; their cow calveth   and</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;casteth not her calf.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:011 They send forth their little ones like a flock   and their</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;children dance.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:012 They take the timbrel and harp   and rejoice at the sound of</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;the organ.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:013 They spend their days in wealth   and in a moment go down to</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;the grave.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:014 Therefore they say unto God   Depart from us; for we desire not</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;the knowledge of thy ways.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:015 What is the Almighty   that we should serve him? and what</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;profit should we have   if we pray unto him?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:016 Lo   their good is not in their hand: the counsel of the wicked</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;is far from me.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:017 How oft is the candle of the wicked put out! and how oft</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;cometh their destruction upon them! God distributeth sorrows</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;in his anger.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:018 They are as stubble before the wind   and as chaff that the</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;storm carrieth away.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:019 God layeth up his iniquity for his children: he rewardeth him</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and he shall know it.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:020 His eyes shall see his destruction   and he shall drink of the</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;wrath of the Almighty.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:021 For what pleasure hath he in his house after him   when the</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;number of his months is cut off in the midst?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:022 Shall any teach God knowledge? seeing he judgeth those that</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;are high.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:023 One dieth in his full strength   being wholly at ease and</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;quiet.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:024 His breasts are full of milk   and his bones are moistened with</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;marrow.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:025 And another dieth in the bitterness of his soul   and never</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;eateth with pleasure.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:026 They shall lie down alike in the dust   and the worms shall</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;cover them.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:027 Behold   I know your thoughts   and the devices which ye</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;wrongfully imagine against me.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:028 For ye say   Where is the house of the prince? and where are</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;the dwelling places of the wicked?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:029 Have ye not asked them that go by the way? and do ye not know</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;their tokens</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:030 That the wicked is reserved to the day of destruction? they</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;shall be brought forth to the day of wrath.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:031 Who shall declare his way to his face? and who shall repay him</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;what he hath done?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:032 Yet shall he be brought to the grave   and shall remain in the</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tomb.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:033 The clods of the valley shall be sweet unto him   and every man</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;shall draw after him   as there are innumerable before him.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>021:034 How then comfort ye me in vain   seeing in your answers there</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;remaineth falsehood?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:001 Then Eliphaz the Temanite answered and said</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:002 Can a man be profitable unto God   as he that is wise may be</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;profitable unto himself?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:003 Is it any pleasure to the Almighty   that thou art righteous?</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;or is it gain to him   that thou makest thy ways perfect?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:004 Will he reprove thee for fear of thee? will he enter with thee</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;into judgment?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:005 Is not thy wickedness great? and thine iniquities infinite?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:006 For thou hast taken a pledge from thy brother for nought   and</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;stripped the naked of their clothing.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:007 Thou hast not given water to the weary to drink   and thou hast</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;withholden bread from the hungry.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:008 But as for the mighty man   he had the earth; and the</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;honourable man dwelt in it.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:009 Thou hast sent widows away empty   and the arms of the</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;fatherless have been broken.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:010 Therefore snares are round about thee   and sudden fear</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;troubleth thee;</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:011 Or darkness   that thou canst not see; and abundance of waters</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;cover thee.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:012 Is not God in the height of heaven? and behold the height of</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;the stars   how high they are!</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:013 And thou sayest   How doth God know? can he judge through the</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;dark cloud?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:014 Thick clouds are a covering to him   that he seeth not; and he</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;walketh in the circuit of heaven.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:015 Hast thou marked the old way which wicked men have trodden?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:016 Which were cut down out of time   whose foundation was</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;overflown with a flood:</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:017 Which said unto God   Depart from us: and what can the Almighty</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;do for them?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:018 Yet he filled their houses with good things: but the counsel</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;of the wicked is far from me.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:019 The righteous see it   and are glad: and the innocent laugh</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;them to scorn.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:020 Whereas our substance is not cut down   but the remnant of them</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;the fire consumeth.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:021 Acquaint now thyself with him   and be at peace: thereby good</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;shall come unto thee.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:022 Receive   I pray thee   the law from his mouth   and lay up his</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;words in thine heart.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:023 If thou return to the Almighty   thou shalt be built up   thou</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;shalt put away iniquity far from thy tabernacles.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:024 Then shalt thou lay up gold as dust   and the gold of Ophir as</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;the stones of the brooks.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:025 Yea   the Almighty shall be thy defence   and thou shalt have</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;plenty of silver.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:026 For then shalt thou have thy delight in the Almighty   and</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;shalt lift up thy face unto God.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:027 Thou shalt make thy prayer unto him   and he shall hear thee</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and thou shalt pay thy vows.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:028 Thou shalt also decree a thing   and it shall be established</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;unto thee: and the light shall shine upon thy ways.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:029 When men are cast down   then thou shalt say   There is lifting</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;up; and he shall save the humble person.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>022:030 He shall deliver the island of the innocent: and it is</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;delivered by the pureness of thine hands.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:001 Then Job answered and said</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:002 Even to day is my complaint bitter: my stroke is heavier than</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;my groaning.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:003 Oh that I knew where I might find him! that I might come even</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;to his seat!</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:004 I would order my cause before him   and fill my mouth with</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;arguments.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:005 I would know the words which he would answer me   and</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;understand what he would say unto me.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:006 Will he plead against me with his great power? No; but he</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;would put strength in me.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:007 There the righteous might dispute with him; so should I be</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;delivered for ever from my judge.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:008 Behold   I go forward   but he is not there; and backward   but I</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;cannot perceive him:</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:009 On the left hand   where he doth work   but I cannot behold him:</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;he hideth himself on the right hand   that I cannot see him:</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:010 But he knoweth the way that I take: when he hath tried me   I</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;shall come forth as gold.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:011 My foot hath held his steps   his way have I kept   and not</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;declined.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:012 Neither have I gone back from the commandment of his lips; I</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;have esteemed the words of his mouth more than my necessary</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;food.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:013 But he is in one mind   and who can turn him? and what his soul</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;desireth   even that he doeth.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:014 For he performeth the thing that is appointed for me: and many</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;such things are with him.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:015 Therefore am I troubled at his presence: when I consider   I am</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;afraid of him.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:016 For God maketh my heart soft   and the Almighty troubleth me:</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>023:017 Because I was not cut off before the darkness   neither hath he</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;covered the darkness from my face.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>024:001 Why   seeing times are not hidden from the Almighty   do they</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;that know him not see his days?</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>024:002 Some remove the landmarks; they violently take away flocks</p>"
        m$ = m$ + "<p>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;and feed thereof.</p>"
        m$ = m$ + "<p>&nbsp;</p>"
        m$ = m$ + "<p>024:003 They drive away the ass of the fatherless   they take the</p>"
