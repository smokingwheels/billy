CONST MAX_CLIENTS = 30000
CONST EXPIRY_TIME = 240 'seconds
CONST MIDNIGHT_FIX_WINDOW = 60 * 60 'seconds
CONST MAX_HEADER_SIZE = 4096 'bytes
CONST DEFAULT_HOST = "192.168.1.10"
CONST METHOD_HEAD = 1
CONST METHOD_GET = 2
CONST METHOD_POST = 3
DIM SHARED CRLF AS STRING
DIM SHARED CONTENT AS STRING
CRLF = CHR$(13) + CHR$(10)
